{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "google": {
    "servicesJson": "GoogleServicesJson",
    "infoPlist": "GoogleServicesInfoPlist"
  },
  "appSettings": {
    "Sample": [
      {
        "delimiter": ";",
        "className": "Values",
        "accessibility": "Public",
        "namespace": "Secrets",
        "rootNamespace": "Sample",
        "properties": [
          {
            "name": "OneSignalAppId",
            "type": "String"
          },
          {
            "name": "AzureNotificationHubsListenerConnectionString",
            "type": "String"
          },
          {
            "name": "AzureNotificationHubsFullConnectionString",
            "type": "String"
          },
          {
            "name": "AzureNotificationHubsHubName",
            "type": "String"
          }
        ]
      }
    ]
  }
}