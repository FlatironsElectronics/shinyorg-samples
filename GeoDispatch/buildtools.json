{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "appConfig": {
    //"disable": true
    "strategy": "TransformOnly"
  },
  "google": {
    "servicesJson": "GoogleServicesJson"
    //"infoPlist": "GoogleServicesInfoPlist"
  },
  "appSettings": {
    "Sample": [
      {
        "delimiter": ";",
        "className": "Values",
        "accessibility": "Public",
        "namespace": "Secrets",
        "rootNamespace": "Sample",
        "properties": [
          {
            "name": "AzureNotificationHubName",
            "type": "String"
          },
          {
            "name": "AzureNotificationHubFullConnectionString",
            "type": "String"
          }
        ]
      }
    ]
  }
}